<h2>注文履歴一覧</h2>

<table>

  <thead>
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>

    <tr>
      <td><%= order.created_at %><td>
      <td><%= order.customer_id.name %></td>
      <td><%= order.order_detail_id.amount %></td>
      <td><%= order.order_status %></td>
    </tr>

    <% end %>
  </tbody>
