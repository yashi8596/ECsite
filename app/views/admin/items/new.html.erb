<h2>商品新規登録</h2>

<%= form_with model: @item, url: admin_items_path, method: :post do |f| %>

  <div>
    <label>商品画像</label>
  </div>

  <div>
    <label>商品名</label>
    <%= f.text_field :name %>
  </div>

  <div>
    <label>商品説明</label>
    <%= f.text_area :introduction %>
  </div>

  <div>
    <label>ジャンル</label>
  </div>

  <div>
    <label>税抜価格</label>
    <%= f.text_field :price %>円
  </div>

  <div>
    <label>販売ステータス</label>
  </div>

  <%= f.submit '新規登録' %>

<% end %>